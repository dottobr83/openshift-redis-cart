#!/bin/bash
redis_url="redis://$OPENSHIFT_REDIS_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_REDIS_DB_PROXY_PORT/"

echo "OPENSHIFT_REDIS_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_REDIS_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_REDIS_DB_PASSWORD=$OPENSHIFT_REDIS_DB_PASSWORD"
echo "OPENSHIFT_REDIS_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_REDIS_DB_PORT=$OPENSHIFT_REDIS_DB_PROXY_PORT"
echo "OPENSHIFT_REDIS_DB_URL=$redis_url"
